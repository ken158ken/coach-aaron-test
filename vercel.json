{
  "buildCommand": "cd frontend && npm install && npm run build:client",
  "outputDirectory": "frontend/dist/client",
  "installCommand": "cd backend && npm install",
  "framework": null,
  "functions": {
    "backend/index.js": {
      "runtime": "nodejs24.x",
      "includeFiles": "backend/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/backend/index.js"
    }
  ]
}
